{
  "manifest_version": 3,
  "name": "CLI for chess.com",
  "description": "Base Level Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["/dist/scripts/content.js"],
      "matches": ["https://www.chess.com/play/*"]
    }
  ],
  "action": {
    "default_title": "CLI for chess.com",
    "default_popup": "./index.html"
  }
}
